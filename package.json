{
  "name": "open-tree-resolver",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "pretest": "vue-cli-service lint",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/phyloref/curation-tool.git"
  },
  "dependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.7.5",
    "@phyloref/phyx": "^0.2.1",
    "bootstrap": "^4.4.1",
    "bootstrap-vue": "^2.1.0",
    "filesaver.js-npm": "^1.0.1",
    "jquery": ">=3.5.0",
    "lodash": "^4.17.15",
    "newick-js": "^1.1.1",
    "popper.js": "^1.16.0",
    "vue": "^2.6.11",
    "vue-resize": "^0.4.5",
    "vuex": "^3.1.2",
    "x-hub-signature": "^1.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.7",
    "@babel/core": "^7.7.7",
    "@babel/preset-env": "^7.7.7",
    "@vue/cli-plugin-babel": "^3.12.1",
    "@vue/cli-plugin-eslint": "^3.12.1",
    "@vue/cli-service": "^3.12.1",
    "@vue/test-utils": "^1.0.0-beta.30",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-json": "^1.3.2",
    "eslint-plugin-mocha": "^5.2.0",
    "eslint-plugin-vue": "^5.2.3",
    "jest": "^24.9.0",
    "vue-jest": "^3.0.5",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true,
      "jest": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "no-console": "warn"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "transform": {
      "^.*\\.vue$": "vue-jest",
      "^.+\\.js$": "babel-jest"
    },
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{js,vue}",
      "!**/node_modules/**",
      "!docs/**"
    ],
    "coverageReporters": [
      "html",
      "text-summary"
    ]
  }
}
